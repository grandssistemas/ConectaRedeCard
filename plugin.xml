<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-conecta-rede-card" version="0.0.1" 
    xmlns="http://apache.org/cordova/ns/plugins/1.0" 
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>ConectaRedeCard</name>
    <js-module name="ConectaRedeCard" src="www/ConectaRedeCard.js">
        <clobbers target="cordova.plugins.ConectaRedeCard" />
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="ConectaRedeCard">
                <param name="android-package" value="cordova-plugin-conecta-rede-card.ConectaRedeCard" />
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/ConectaRedeCard.java" target-dir="src/cordova-plugin-conecta-rede-card/ConectaRedeCard" />
        <source-file src="src/libs/sdk-1.0-release.aar" target-dir="libs" framework="true" />
        <framework src="src/android/build.gradle" custom="true" type="gradleReference" />
    </platform>
</plugin>